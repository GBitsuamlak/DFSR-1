FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      DFSRTurbDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../setUp"

patchName               "inlet";

startTime 		0.0;
endTime                 1.0;
timeStep		0.002;
fMax                    250;
nFreq			1024;
nInterp			30;
cohUav                  $Uref;           
seed                    1515;          

C                       (
                          3.00 10.0 10.0
                          3.00 4.00 5.50
                          0.50 6.50 3.00
                        );  

scaleI                  (1.0 1.0 1.0);
scaleL                  (1.0 1.0 1.0);

correctDivergence	on;
writeSamples           	on;
writeInflow            	on;
